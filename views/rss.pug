extends layout

block content
  .page-header
    h3 Crawling RSS

  form.form-horizontal(method='POST')
    //input(type='hidden', name='_csrf', value=_csrf)
    .form-group
      label(class='col-sm-2 control-label', for='rss') Rss
      .col-sm-8
        input.form-control(type='text', name='rss', id='rss', autofocus=true)
    .form-group
      .col-sm-offset-2.col-sm-8
        button.btn.btn-primary(type='submit')
          i.fa.fa-envelope
          | Send
